type KYCInfo = record {
  status : bool;
  update_time : nat64;
  expire_time : nat64;
};
service : {
  add_kyc : (nat64, principal, nat64, KYCInfo) -> ();
  balance_of : (principal) -> (nat64);
  claim : (text) -> ();
  get_base_uri : () -> (text);
  get_kyc_info : (nat64, principal, nat64) -> (KYCInfo);
  owner_of : (nat64) -> (principal);
  token_id_to_did : (nat64) -> (opt text);
  token_uri : (nat64) -> (opt text);
}